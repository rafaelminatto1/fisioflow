# Configurações de Produção - FisioFlow
# ATENÇÃO: Não commitar este arquivo com valores reais

# Environment (NODE_ENV será definido automaticamente pelo Vite)
VITE_BUILD_MODE=production

# Application Settings
VITE_APP_NAME=FisioFlow
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://fisioflow.vercel.app

# Gemini AI (Opcional - para funcionalidades de IA)
VITE_GEMINI_API_KEY=your_production_gemini_key_here

# Features em Produção
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true

# Segurança
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true
VITE_DISABLE_DEV_TOOLS=true

# Analytics (Opcional)
VITE_GA_TRACKING_ID=GA_TRACKING_ID_HERE
VITE_HOTJAR_ID=HOTJAR_ID_HERE

# Monitoramento (Opcional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production

# Build Optimization
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_MINIFICATION=true
VITE_ENABLE_TREE_SHAKING=true