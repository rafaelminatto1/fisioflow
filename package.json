{
  "name": "fisioflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "build:check": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit --skipLibCheck && vite build",
    "build:deploy": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build --config vite.config.deploy.ts",
    "build:minimal": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build --config vite.config.minimal.ts --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "eslint . --ext ts,tsx --fix --max-warnings 50",
    "lint:incremental": "eslint --cache --ext ts,tsx --report-unused-disable-directives --max-warnings 50 .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "node scripts/test-runner.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:integration:watch": "jest --testPathPattern=integration --watch",
    "test:hooks": "jest --testPathPattern=hooks",
    "test:components": "jest --testPathPattern=components",
    "test:utils": "jest --testPathPattern=utils",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage:full": "node scripts/test-runner.js --coverage",
    "test:ci": "node scripts/test-runner.js --ci",
    "test:debug": "node scripts/test-runner.js --debug",
    "type-check": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit --incremental",
    "type-check:fast": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsc --noEmit --skipLibCheck --incremental",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "analyze": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build && npx vite-bundle-analyzer dist",
    "analyze:serve": "npx vite-bundle-analyzer dist --open",
    "clean": "rimraf dist build .tsbuildinfo node_modules/.cache",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@stripe/stripe-js": "^2.1.11",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/file-saver": "^2.0.7",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.4",
    "qrcode.js": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-joyride": "^2.9.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.18.0",
    "react-window": "^1.8.11",
    "recharts": "^2.8.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwindcss": "^3.3.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fake-indexeddb": "^6.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 10"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 5"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
